#!/bin/sh

# -----------------------------------
# Run the PostGRID collector.
# -----------------------------------

# Initialization common to all scripts
BASE_DIR=`dirname $0`/..
export BASE_DIR
. $BASE_DIR/etc/setenv.sh

# Runs Java commands
CLASSPATH=$JARS/postgrid-$CONSTELLATION_VERSION.jar
export CLASSPATH
java $OPTS net.seagis.console.Collector "$@"
