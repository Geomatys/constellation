					<xsd:element ref="gml:coverageFunction" minOccurs="0"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- =========================================================== -->
	<xsd:element name="multiSurfaceDomain" type="gml:MultiSurfaceDomainType" substitutionGroup="gml:domainSet"/>
	<!-- =========================================================== -->
	<xsd:complexType name="MultiSurfaceDomainType">
		<xsd:complexContent>
			<xsd:restriction base="gml:DomainSetType">
				<xsd:sequence minOccurs="0">
					<xsd:element ref="gml:MultiSurface"/>
				</xsd:sequence>
				<xsd:attributeGroup ref="gml:AssociationAttributeGroup"/>
