			<xsd:documentation>A discrete coverage type whose domain is defined by a collection of Solids.</xsd:documentation>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:restriction base="gml:AbstractDiscreteCoverageType">
				<xsd:sequence>
					<xsd:group ref="gml:StandardObjectProperties"/>
					<xsd:element ref="gml:boundedBy" minOccurs="0"/>
					<xsd:element ref="gml:multiSolidDomain"/>
					<xsd:element ref="gml:rangeSet"/>
					<xsd:element ref="gml:coverageFunction" minOccurs="0"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- =========================================================== -->
	<xsd:element name="multiSolidDomain" type="gml:MultiSolidDomainType" substitutionGroup="gml:domainSet"/>
	<!-- =========================================================== -->
	<xsd:complexType name="MultiSolidDomainType">
		<xsd:complexContent>
			<xsd:restriction base="gml:DomainSetType">
				<xsd:sequence minOccurs="0">
					<xsd:element ref="gml:MultiSolid"/>
				</xsd:sequence>
				<xsd:attributeGroup ref="gml:AssociationAttributeGroup"/>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- =========================================================== -->
	<xsd:element name="GridCoverage" type="gml:GridCoverageType" substitutionGroup="gml:_DiscreteCoverage"/>
	<!-- =========================================================== -->
	<xsd:complexType name="GridCoverageType">
		<xsd:complexContent>
			<xsd:restriction base="gml:AbstractDiscreteCoverageType">
				<xsd:sequence>
					<xsd:group ref="gml:StandardObjectProperties"/>
					<xsd:element ref="gml:boundedBy" minOccurs="0"/>
					<xsd:element ref="gml:gridDomain"/>
					<xsd:element ref="gml:rangeSet"/>
					<xsd:element ref="gml:coverageFunction" minOccurs="0"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
	</xsd:complexType>
	<!-- =========================================================== -->
	<xsd:element name="gridDomain" type="gml:GridDomainType" substitutionGroup="gml:domainSet"/>
	<!-- =========================================================== -->
	<xsd:complexType name="GridDomainType">
		<xsd:complexContent>
			<xsd:restriction base="gml:DomainSetType">
