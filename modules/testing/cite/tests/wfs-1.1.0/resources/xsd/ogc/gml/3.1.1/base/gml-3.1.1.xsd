					<xsd:element ref="gml:multiPointDomain"/>
					<xsd:element ref="gml:rangeSet"/>
					<xsd:element ref="gml:coverageFunction" minOccurs="0"/>
				</xsd:sequence>
			</xsd:restriction>
		</xsd:complexContent>
