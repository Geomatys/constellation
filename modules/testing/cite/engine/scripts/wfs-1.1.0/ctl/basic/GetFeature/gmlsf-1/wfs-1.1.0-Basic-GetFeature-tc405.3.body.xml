<?xml version="1.0" encoding="UTF-8"?>
<wfs:GetFeature xmlns:wfs="http://www.opengis.net/wfs" xmlns:gml="http://www.opengis.net/gml" xmlns:ogc="http://www.opengis.net/ogc" version="1.1.0" service="WFS">
	<wfs:Query xmlns:sf="http://cite.opengeospatial.org/gmlsf" typeName="sf:ComplexGeoFeature" srsName="urn:x-ogc:def:crs:EPSG:4326">
		<ogc:Filter>
			<ogc:PropertyIsEqualTo>
				<ogc:PropertyName>sf:observation</ogc:PropertyName>
				<ogc:Literal>2.4</ogc:Literal>
			</ogc:PropertyIsEqualTo>
		</ogc:Filter>
	</wfs:Query>
</wfs:GetFeature>
