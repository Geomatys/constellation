            <gml:lowerCorner>36.0 -1.0</gml:lowerCorner>
            <gml:upperCorner>42.0 4.0</gml:upperCorner>
          </gml:Envelope>
        </ogc:BBOX>
        <ogc:PropertyIsGreaterThan>
          <ogc:PropertyName>sf:dateProperty</ogc:PropertyName>
          <ogc:Literal>2006-10-01Z</ogc:Literal>
        </ogc:PropertyIsGreaterThan>
      </ogc:And>
    </ogc:Filter>
  </wfs:Query>
