<?xml version="1.0" encoding="UTF-8"?>
<cstl:LayerContext xmlns:cstl="http://www.constellation.org/config" xmlns:geotk="cite">
    <cstl:layers>
        <cstl:Source id="shapeSrc" load_all="true">
            <cstl:include>
                <cstl:Layer name="geotk:BasicPolygons">
                    <cstl:Style>${providerStyleType|sldSrc|cite_style_BasicPolygons}</cstl:Style>
                </cstl:Layer>
                <cstl:Layer name="geotk:Bridges">
                    <cstl:Style>${providerStyleType|sldSrc|cite_style_Bridges}</cstl:Style>
                </cstl:Layer>
                <cstl:Layer name="geotk:BuildingCenters">
                    <cstl:Style>${providerStyleType|sldSrc|cite_style_BuildingCenters}</cstl:Style>
                </cstl:Layer>
                <cstl:Layer name="geotk:Buildings">
                    <cstl:Style>${providerStyleType|sldSrc|cite_style_Buildings}</cstl:Style>
                </cstl:Layer>
                <cstl:Layer name="geotk:DividedRoutes">
                    <cstl:Style>${providerStyleType|sldSrc|cite_style_DividedRoutes}</cstl:Style>
                </cstl:Layer>
                <cstl:Layer name="geotk:Forests">
                    <cstl:Style>${providerStyleType|sldSrc|cite_style_Forests}</cstl:Style>
                </cstl:Layer>
                <cstl:Layer name="geotk:Lakes">
                    <cstl:Style>${providerStyleType|sldSrc|cite_style_Lakes}</cstl:Style>
                </cstl:Layer>
                <cstl:Layer name="geotk:MapNeatline">
                    <cstl:Style>${providerStyleType|sldSrc|cite_style_MapNeatLine}</cstl:Style>
                </cstl:Layer>
                <cstl:Layer name="geotk:NamedPlaces">
                    <cstl:Style>${providerStyleType|sldSrc|cite_style_NamedPlaces}</cstl:Style>
                </cstl:Layer>
                <cstl:Layer name="geotk:Ponds">
                    <cstl:Style>${providerStyleType|sldSrc|cite_style_Ponds}</cstl:Style>
                </cstl:Layer>
                <cstl:Layer name="geotk:RoadSegments">
                    <cstl:Style>${providerStyleType|sldSrc|cite_style_RoadSegments}</cstl:Style>
                </cstl:Layer>
                <cstl:Layer name="geotk:Streams">
                    <cstl:Style>${providerStyleType|sldSrc|cite_style_Streams}</cstl:Style>
                </cstl:Layer>
            </cstl:include>
        </cstl:Source>
    </cstl:layers>
    <cstl:customParameters>
        <entry>
            <key>shiroAccessible</key>
            <value>false</value>
        </entry>
    </cstl:customParameters>
</cstl:LayerContext>

