        <rim:Association id="urn:uuid:xxxxxx" associationType="urn:oasis:names:tc:ebxml-regrep:AssociationType:Uses" sourceObject="urn:motiive:csw-ebrim" targetObject="urn:x-ogc:specification:csw-ebrim:package:Basic"/>
        <rim:AdhocQuery id="urn:motiive:csw-ebrim:Query:getFeatureTypes">
            <rim:Slot name="searchid" slotType="URN">
                <rim:ValueList/>
            </rim:Slot>
            <rim:Name>
                <rim:LocalizedString charset="URF-8" xml:lang="en" value="getFeatureTypes"/>
            </rim:Name>
            <rim:Description>
                <rim:LocalizedString charset="URF-8" xml:lang="en" value="Takes as a parameter the id of a feature type. Returns: Feature type records that have that id. This is version dependent - id refers to a specific version of a feature type. This returns only the feature type. Other queries are used to return associated attributes, operations, etc."/>
            </rim:Description>
            <rim:QueryExpression queryLanguage="urn:x-ogc:specification:csw-ebrim:QueryLanguage:CSWQuery" xmlns:csw="http://www.opengis.net/cat/csw/2.0.2">
                <csw:GetRecords>
                    <csw:Query typeNames="csw:ExtrinsicObject csw:ClassificationNode">
                        <csw:ElementSetName typeNames="csw:ExtrinsicObject">full</csw:ElementSetName>
                        <csw:Constraint version="1.1.0">
                            <ogc:Filter>
                                <ogc:And>
                                    <ogc:PropertyIsEqualTo>
                                        <ogc:PropertyName>/ExtrinsicObject/@objectType</ogc:PropertyName>
                                        <ogc:Literal>urn:motiive:csw-ebrim:ObjectType:ebFC_FeatureType</ogc:Literal>
                                    </ogc:PropertyIsEqualTo>
                                    <ogc:PropertyIsEqualTo>
                                        <ogc:PropertyName>/ExtrinsicObject/@id</ogc:PropertyName>
                                        <ogc:Literal>${searchid}</ogc:Literal>
                                    </ogc:PropertyIsEqualTo>
                                </ogc:And>
                            </ogc:Filter>
                        </csw:Constraint>
                    </csw:Query>
                </csw:GetRecords>
            </rim:QueryExpression>
        </rim:AdhocQuery>
        <rim:AdhocQuery xmlns:rim="urn:oasis:names:tc:ebxml-regrep:xsd:rim:3.0" id="urn:motiive:csw-ebrim:Query:getFeatureTypeAttributes">
            <rim:Slot name="searchid" slotType="URN">
                <rim:ValueList/>
            </rim:Slot>
            <rim:Name>
                <rim:LocalizedString charset="URF-8" xml:lang="en" value="getFeatureTypeAttributes"/>
            </rim:Name>
            <rim:Description>
                <rim:LocalizedString charset="URF-8" xml:lang="en" value="Takes as a parameter the id of a feature type. Returns: Feature Attribute records for feature types that have that id. This is version dependent - id refers to a specific version of a feature type. This query may not be very efficient, because there are so many joins and I don't know how the query is executed."/>
            </rim:Description>
            <rim:QueryExpression queryLanguage="urn:x-ogc:specification:csw-ebrim:QueryLanguage:CSWQuery" xmlns:csw="http://www.opengis.net/cat/csw/2.0.2" xmlns:ogc="http://www.opengis.net/ogc">
                <csw:GetRecords>
                    <csw:Query typeNames="csw:ExtrinsicObject=ft, csw:pt, csw:fa csw:ClassificationNode=cnft, csw:cnpt, csw:cnfa">
                        <csw:ElementSetName typeNames="csw:ExtrinsicObject=fa">full</csw:ElementSetName>
                        <csw:Constraint version="1.1.0">
                            <ogc:Filter>
                                <ogc:And>
                                    <ogc:PropertyIsEqualTo>
                                        <ogc:PropertyName>/ft@objectType</ogc:PropertyName>
                                        <ogc:Literal>/cnft/@id</ogc:Literal>
                                    </ogc:PropertyIsEqualTo>
                                    <ogc:PropertyIsEqualTo>
                                        <ogc:PropertyName>/cnft/@id</ogc:PropertyName>
                                        <ogc:Literal>urn:motiive:csw-ebrim:ObjectType:ebFC_FeatureType</ogc:Literal>
                                    </ogc:PropertyIsEqualTo>
                                    <ogc:PropertyIsEqualTo>
                                        <ogc:PropertyName>/ft/@id</ogc:PropertyName>
                                        <ogc:Literal>${searchid}</ogc:Literal>
                                    </ogc:PropertyIsEqualTo>
                                    <ogc:PropertyIsEqualTo>
                                        <ogc:PropertyName>/pt/@objectType</ogc:PropertyName>
                                        <ogc:Literal>/cnpt/@id</ogc:Literal>
                                    </ogc:PropertyIsEqualTo>
                                    <ogc:PropertyIsEqualTo>
                                        <ogc:PropertyName>/cnpt/@id</ogc:PropertyName>
                                        <ogc:Literal>urn:motiive:csw-ebrim:ObjectType:ebFC_PropertyType</ogc:Literal>
                                    </ogc:PropertyIsEqualTo>
                                    <ogc:PropertyIsEqualTo>
                                        <ogc:PropertyName>/ft/@id</ogc:PropertyName>
                                        <ogc:Literal>/pt/@id</ogc:Literal>
                                    </ogc:PropertyIsEqualTo>
                                    <ogc:PropertyIsEqualTo>
                                        <ogc:PropertyName>/fa/@objectType</ogc:PropertyName>
                                        <ogc:Literal>/cnfa/@id</ogc:Literal>
                                    </ogc:PropertyIsEqualTo>
                                    <ogc:PropertyIsEqualTo>
                                        <ogc:PropertyName>/cnfa/@id</ogc:PropertyName>
                                        <ogc:Literal>urn:motiive:csw-ebrim:ObjectType:ebFC_FeatureAttribute</ogc:Literal>
                                    </ogc:PropertyIsEqualTo>
                                    <ogc:PropertyIsEqualTo>
                                        <ogc:PropertyName>/fa/@id</ogc:PropertyName>
                                        <ogc:Literal>/pt/@id</ogc:Literal>
                                    </ogc:PropertyIsEqualTo>
                                </ogc:And>
                            </ogc:Filter>
                        </csw:Constraint>
                    </csw:Query>
                </csw:GetRecords>
            </rim:QueryExpression>
        </rim:AdhocQuery>
