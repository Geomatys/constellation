<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="version_1.36" author="glegal">
        <createTable schemaName="admin" tableName="metadata_bbox">
            <column name="metadata_id" type="INTEGER" >
                <constraints nullable="false" />
            </column>
            <column name="east" type="DOUBLE" >
                <constraints nullable="false" />
            </column>
            <column name="west" type="DOUBLE" >
                <constraints nullable="false" />
            </column>
            <column name="north" type="DOUBLE" >
                <constraints nullable="false" />
            </column>
            <column name="south" type="DOUBLE" >
                <constraints nullable="false" />
            </column>
        </createTable>
        <addForeignKeyConstraint
            baseTableSchemaName="admin" referencedTableSchemaName="admin"
            constraintName="bbox_metadata_fk"
            baseTableName="metadata_bbox" baseColumnNames="metadata_id"
            referencedTableName="metadata" referencedColumnNames="id"
            onDelete="CASCADE" />
        <addPrimaryKey columnNames="metadata_id, east, west, north, south"
                       constraintName="METADATA_BBOX_PK"
                       schemaName="admin"
                       tableName="metadata_bbox"/>
        
    </changeSet>
</databaseChangeLog>