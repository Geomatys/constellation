<?xml version="1.0" encoding="UTF-8"?>
<cstl:LayerContext xmlns:cstl="http://www.constellation.org/config" xmlns:gml="http://www.opengis.net/gml">
    <cstl:supportedLanguages>
       <cstl:language>fre</cstl:language>
       <cstl:language default="true">eng</cstl:language>
    </cstl:supportedLanguages>
    <cstl:layers>
        <cstl:Source id="coverageTestSrc" load_all="true">
           <cstl:exclude>
                <cstl:Layer name="SST_tests"/>
           </cstl:exclude>
        </cstl:Source>
        <cstl:Source id="shapeSrc">
           <cstl:include>
                <cstl:Layer name="gml:Lakes"/>
           </cstl:include>
        </cstl:Source>
        <cstl:Source id="postgisSrc" load_all="true"/>
    </cstl:layers>
    <cstl:customParameters>
        <entry>
            <key>shiroAccessible</key>
            <value>false</value>
        </entry>
    </cstl:customParameters>
</cstl:LayerContext>

