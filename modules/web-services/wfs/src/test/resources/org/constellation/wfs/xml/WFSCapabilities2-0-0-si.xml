<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns8:WFS_Capabilities version="2.0.0" xmlns:fes="http://www.opengis.net/fes/2.0" xmlns:ogc="http://www.opengis.net/ogc" xmlns:wfs="http://www.opengis.net/wfs" xmlns:ins="http://www.inspire.org" xmlns:ows="http://www.opengis.net/ows" xmlns:ns9="http://www.opengis.net/ows/1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gml="http://www.opengis.net/gml" xmlns:ns8="http://www.opengis.net/wfs/2.0">
    <ns9:ServiceIdentification>
        <ns9:Title>Web Feature Service (Constellation)</ns9:Title>
        <ns9:Abstract>Features provided by constellation SDI server.</ns9:Abstract>
        <ns9:Keywords>
            <ns9:Keyword>WFS</ns9:Keyword>
            <ns9:Keyword>Constellation</ns9:Keyword>
            <ns9:Keyword>Geomatys</ns9:Keyword>
        </ns9:Keywords>
        <ns9:ServiceType>WFS</ns9:ServiceType>
        <ns9:ServiceTypeVersion>2.0.0</ns9:ServiceTypeVersion>
        <ns9:ServiceTypeVersion>1.1.0</ns9:ServiceTypeVersion>
        <ns9:Fees>NONE</ns9:Fees>
        <ns9:AccessConstraints>NONE</ns9:AccessConstraints>
    </ns9:ServiceIdentification>
    <fes:Filter_Capabilities>
        <fes:Conformance>
            <fes:Constraint name="ImplementsQuery">
                <ns9:NoValues/>
                <ns9:DefaultValue>TRUE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsAdHocQuery">
                <ns9:NoValues/>
                <ns9:DefaultValue>TRUE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsFunctions">
                <ns9:NoValues/>
                <ns9:DefaultValue>FALSE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsMinStandardFilter">
                <ns9:NoValues/>
                <ns9:DefaultValue>TRUE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsStandardFilter">
                <ns9:NoValues/>
                <ns9:DefaultValue>TRUE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsMinSpatialFilter">
                <ns9:NoValues/>
                <ns9:DefaultValue>TRUE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsSpatialFilter">
                <ns9:NoValues/>
                <ns9:DefaultValue>TRUE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsMinTemporalFilter">
                <ns9:NoValues/>
                <ns9:DefaultValue>TRUE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsTemporalFilter">
                <ns9:NoValues/>
                <ns9:DefaultValue>TRUE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsVersionNav">
                <ns9:NoValues/>
                <ns9:DefaultValue>FALSE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsSorting">
                <ns9:NoValues/>
                <ns9:DefaultValue>TRUE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsExtendedOperators">
                <ns9:NoValues/>
                <ns9:DefaultValue>FALSE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsResourceId">
                <ns9:NoValues/>
                <ns9:DefaultValue>TRUE</ns9:DefaultValue>
            </fes:Constraint>
            <fes:Constraint name="ImplementsMinimumXPath">
                <ns9:NoValues/>
                <ns9:DefaultValue>TRUE</ns9:DefaultValue>
            </fes:Constraint>
        </fes:Conformance>
        <fes:Id_Capabilities>
            <fes:ResourceIdentifier name="fes:ResourceId"/>
        </fes:Id_Capabilities>
        <fes:Scalar_Capabilities>
            <fes:LogicalOperators/>
            <fes:ComparisonOperators>
                <fes:ComparisonOperator name="PropertyIsBetween"/>
                <fes:ComparisonOperator name="PropertyIsEqualTo"/>
                <fes:ComparisonOperator name="PropertyIsGreaterThan"/>
                <fes:ComparisonOperator name="PropertyIsGreaterThanOrEqualTo"/>
                <fes:ComparisonOperator name="PropertyIsLessThan"/>
                <fes:ComparisonOperator name="PropertyIsLessThanOrEqualTo"/>
                <fes:ComparisonOperator name="PropertyIsLike"/>
                <fes:ComparisonOperator name="PropertyIsNotEqualTo"/>
                <fes:ComparisonOperator name="PropertyIsNull"/>
            </fes:ComparisonOperators>
        </fes:Scalar_Capabilities>
        <fes:Spatial_Capabilities>
            <fes:GeometryOperands>
                <fes:GeometryOperand name="gml:Envelope"/>
                <fes:GeometryOperand name="gml:Point"/>
                <fes:GeometryOperand name="gml:LineString"/>
                <fes:GeometryOperand name="gml:Polygon"/>
            </fes:GeometryOperands>
            <fes:SpatialOperators>
                <fes:SpatialOperator name="Disjoint"/>
                <fes:SpatialOperator name="Equals"/>
                <fes:SpatialOperator name="DWithin"/>
                <fes:SpatialOperator name="Beyond"/>
                <fes:SpatialOperator name="Intersects"/>
                <fes:SpatialOperator name="Touches"/>
                <fes:SpatialOperator name="Crosses"/>
                <fes:SpatialOperator name="Contains"/>
                <fes:SpatialOperator name="Overlaps"/>
                <fes:SpatialOperator name="BBOX"/>
            </fes:SpatialOperators>
        </fes:Spatial_Capabilities>
    </fes:Filter_Capabilities>
</ns8:WFS_Capabilities>