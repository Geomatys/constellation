<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns8:WFS_Capabilities version="2.0.0" xmlns:fes="http://www.opengis.net/fes/2.0" xmlns:ogc="http://www.opengis.net/ogc" xmlns:wfs="http://www.opengis.net/wfs" xmlns:ins="http://www.inspire.org" xmlns:ows="http://www.opengis.net/ows" xmlns:ns9="http://www.opengis.net/ows/1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gml="http://www.opengis.net/gml" xmlns:ns8="http://www.opengis.net/wfs/2.0">
    <ns9:OperationsMetadata>
        <ns9:Operation name="GetCapabilities">
            <ns9:DCP>
                <ns9:HTTP>
                    <ns9:Get xlink:href="http://geomatys.com/constellation/WS/wfs/default?"/>
                    <ns9:Post xlink:href="http://geomatys.com/constellation/WS/wfs/default?"/>
                </ns9:HTTP>
            </ns9:DCP>
            <ns9:Parameter name="AcceptVersions">
                <ns9:AllowedValues>
                    <ns9:Value>2.0.0</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
            <ns9:Parameter name="AcceptFormats">
                <ns9:AllowedValues>
                    <ns9:Value>text/xml</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
            <ns9:Parameter name="Service">
                <ns9:AllowedValues>
                    <ns9:Value>WFS</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
        </ns9:Operation>
        <ns9:Operation name="DescribeFeatureType">
            <ns9:DCP>
                <ns9:HTTP>
                    <ns9:Get xlink:href="http://geomatys.com/constellation/WS/wfs/default?"/>
                    <ns9:Post xlink:href="http://geomatys.com/constellation/WS/wfs/default?"/>
                </ns9:HTTP>
            </ns9:DCP>
            <ns9:Parameter name="outputFormat">
                <ns9:AllowedValues>
                    <ns9:Value>text/xml; subtype=gml/3.1.1</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
            <ns9:Parameter name="Service">
                <ns9:AllowedValues>
                    <ns9:Value>WFS</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
            <ns9:Parameter name="Version">
                <ns9:AllowedValues>
                    <ns9:Value>2.0.0</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
        </ns9:Operation>
        <ns9:Operation name="GetFeature">
            <ns9:DCP>
                <ns9:HTTP>
                    <ns9:Get xlink:href="http://geomatys.com/constellation/WS/wfs/default?"/>
                    <ns9:Post xlink:href="http://geomatys.com/constellation/WS/wfs/default?"/>
                </ns9:HTTP>
            </ns9:DCP>
            <ns9:Parameter name="resultType">
                <ns9:AllowedValues>
                    <ns9:Value>results</ns9:Value>
                    <ns9:Value>hits</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
            <ns9:Parameter name="outputFormat">
                <ns9:AllowedValues>
                    <ns9:Value>text/xml; subtype=gml/3.1.1</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
            <ns9:Parameter name="Service">
                <ns9:AllowedValues>
                    <ns9:Value>WFS</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
            <ns9:Parameter name="Version">
                <ns9:AllowedValues>
                    <ns9:Value>2.0.0</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
            <ns9:Constraint name="LocalTraverseXLinkScope">
                <ns9:AllowedValues>
                    <ns9:Value>2</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Constraint>
        </ns9:Operation>
        <ns9:Operation name="Transaction">
            <ns9:DCP>
                <ns9:HTTP>
                    <ns9:Post xlink:href="http://geomatys.com/constellation/WS/wfs/default?"/>
                </ns9:HTTP>
            </ns9:DCP>
            <ns9:Parameter name="inputFormat">
                <ns9:AllowedValues>
                    <ns9:Value>text/xml; subtype=gml/3.1.1</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
            <ns9:Parameter name="idgen">
                <ns9:AllowedValues>
                    <ns9:Value>GenerateNew</ns9:Value>
                    <ns9:Value>UseExisting</ns9:Value>
                    <ns9:Value>ReplaceDuplicate</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
            <ns9:Parameter name="releaseAction">
                <ns9:AllowedValues>
                    <ns9:Value>ALL</ns9:Value>
                    <ns9:Value>SOME</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
            <ns9:Parameter name="Service">
                <ns9:AllowedValues>
                    <ns9:Value>WFS</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
            <ns9:Parameter name="Version">
                <ns9:AllowedValues>
                    <ns9:Value>2.0.0</ns9:Value>
                </ns9:AllowedValues>
            </ns9:Parameter>
        </ns9:Operation>
    </ns9:OperationsMetadata>
    <fes:Filter_Capabilities>
        <fes:Id_Capabilities>
            <fes:ResourceIdentifier name="fes:ResourceId"/>
        </fes:Id_Capabilities>
        <fes:Scalar_Capabilities>
            <fes:LogicalOperators/>
            <fes:ComparisonOperators>
                <fes:ComparisonOperator name="PropertyIsBetween"/>
                <fes:ComparisonOperator name="PropertyIsEqualTo"/>
                <fes:ComparisonOperator name="PropertyIsGreaterThan"/>
                <fes:ComparisonOperator name="PropertyIsGReaterThanEqualTo"/>
                <fes:ComparisonOperator name="PropertyIsLessThan"/>
                <fes:ComparisonOperator name="PropertyIsLessThanEqualTo"/>
                <fes:ComparisonOperator name="PropertyIsLike"/>
                <fes:ComparisonOperator name="PropertyIsNotEqualTo"/>
                <fes:ComparisonOperator name="PropertyIsNull"/>
            </fes:ComparisonOperators>
        </fes:Scalar_Capabilities>
        <fes:Spatial_Capabilities>
            <fes:GeometryOperands>
                <fes:GeometryOperand name="ns11:Envelope" xmlns:ns11="http://www.opengis.net/gml/3.2.1"/>
                <fes:GeometryOperand name="ns11:Point" xmlns:ns11="http://www.opengis.net/gml/3.2.1"/>
                <fes:GeometryOperand name="ns11:LineString" xmlns:ns11="http://www.opengis.net/gml/3.2.1"/>
                <fes:GeometryOperand name="ns11:Polygon" xmlns:ns11="http://www.opengis.net/gml/3.2.1"/>
            </fes:GeometryOperands>
            <fes:SpatialOperators>
                <fes:SpatialOperator name="DISJOINT"/>
                <fes:SpatialOperator name="EQUALS"/>
                <fes:SpatialOperator name="D_WITHIN"/>
                <fes:SpatialOperator name="BEYOND"/>
                <fes:SpatialOperator name="INTERSECTS"/>
                <fes:SpatialOperator name="TOUCHES"/>
                <fes:SpatialOperator name="CROSSES"/>
                <fes:SpatialOperator name="CONTAINS"/>
                <fes:SpatialOperator name="OVERLAPS"/>
                <fes:SpatialOperator name="BBOX"/>
            </fes:SpatialOperators>
        </fes:Spatial_Capabilities>
    </fes:Filter_Capabilities>
</ns8:WFS_Capabilities>
