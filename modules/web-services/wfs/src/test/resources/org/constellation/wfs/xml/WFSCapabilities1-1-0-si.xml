<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<wfs:WFS_Capabilities version="1.1.0" xmlns:fes="http://www.opengis.net/fes/2.0" xmlns:ogc="http://www.opengis.net/ogc" xmlns:wfs="http://www.opengis.net/wfs" xmlns:ins="http://www.inspire.org" xmlns:ows="http://www.opengis.net/ows" xmlns:ns9="http://www.opengis.net/ows/1.1" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:gml="http://www.opengis.net/gml" xmlns:ns8="http://www.opengis.net/wfs/2.0">
    <ows:ServiceIdentification>
        <ows:Title>Web Feature Service (Constellation)</ows:Title>
        <ows:Abstract>Features provided by constellation SDI server.</ows:Abstract>
        <ows:Keywords>
            <ows:Keyword>WFS</ows:Keyword>
            <ows:Keyword>Constellation</ows:Keyword>
            <ows:Keyword>Geomatys</ows:Keyword>
        </ows:Keywords>
        <ows:ServiceType>WFS</ows:ServiceType>
        <ows:ServiceTypeVersion>2.0.0</ows:ServiceTypeVersion>
        <ows:ServiceTypeVersion>1.1.0</ows:ServiceTypeVersion>
        <ows:Fees>NONE</ows:Fees>
        <ows:AccessConstraints>NONE</ows:AccessConstraints>
