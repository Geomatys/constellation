  -->
    <dependencies>
        <dependency>
            <groupId>org.constellation.storage</groupId>
            <artifactId>cstl-store-provider</artifactId>
            <version>${project.version}</version>
        </dependency>
        <dependency>
            <groupId>org.constellation.gui</groupId>
            <artifactId>admin-base</artifactId>
            <version>${project.version}</version>
        </dependency>

        <!-- Web interface -->
        <dependency>
            <groupId>org.constellation.gui</groupId>
            <artifactId>admin-base</artifactId>
            <version>${project.version}</version>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mapfaces</groupId>
            <artifactId>mapfaces-core</artifactId>
            <scope>provided</scope>
        </dependency>
        <dependency>
            <groupId>org.mapfaces</groupId>
            <artifactId>mapfaces-io</artifactId>
        </dependency>

        <dependency>
            <groupId>javax.el</groupId>
            <artifactId>el-api</artifactId>
            <scope>provided</scope>
        </dependency>

    </dependencies>

</project>






