    <name>Web site : WCS informations</name>


    <!--
    Developers and contributors.
  -->
    <developers>
        <developer>
            <id>glegal</id>
            <name>Guilhem Legal</name>
            <email>guilhem.legal@geomatys.com</email>
            <organization>Geomatys</organization>
