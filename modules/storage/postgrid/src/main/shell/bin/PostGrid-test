#!/bin/sh

# ---------------------------------------------------------------------------
# Tests the connection to PostGrid database. The ".seagis/config.xml" file
# must be presents. This script do not expect any argument.
# ---------------------------------------------------------------------------

# Initialization common to all scripts
BASE_DIR=`dirname $0`/..
export BASE_DIR
. $BASE_DIR/etc/setenv.sh

# Runs Java commands
export CLASSPATH=$JARS/postgrid-$CONSTELLATION_VERSION.jar
java $OPTS -showversion org.constellation.catalog.Database "$@"
