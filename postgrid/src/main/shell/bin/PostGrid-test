#!/bin/sh

# -------------------------------------------------
# Teste la connexion à la base de données PostGrid.
# -------------------------------------------------

# Initialisation commune à tous les scripts
export BASE_DIR=`dirname $0`/..
source $BASE_DIR/etc/setenv.sh

# Exécution de la commande Java
export CLASSPATH=$JARS/postgrid-$SEAGIS_VERSION.jar
java $OPTS -showversion net.seagis.catalog.Database $*
