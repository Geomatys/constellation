  <Name>WMS</Name>
  <!-- Human-readable title for pick lists -->
  <Title>SeaGis Map Server</Title>
  <!-- Narrative description providing additional information -->
  <Abstract>Map Server.  Contact: someone@geomatys.fr.  High-quality maps.</Abstract>
  <KeywordList>
    <Keyword>WMS</Keyword>
    <Keyword>???</Keyword>
    <Keyword>????</Keyword>
  </KeywordList>
  <!-- Top-level web address of service or service provider.  See also OnlineResource
  elements under <DCPType>. -->
  <OnlineResource xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple" xlink:href="http://sensor.geomatys.fr/"/>
  <!-- Contact information -->
  <ContactInformation>
    <ContactPersonPrimary>
      <ContactPerson>vincent Heurteaux</ContactPerson>
      <ContactOrganization>Geomatys</ContactOrganization>
    </ContactPersonPrimary>
    <ContactPosition>PDG</ContactPosition>
    <ContactAddress>
      <AddressType>postal</AddressType>
      <Address>24 rue Pierre Renaudel</Address>
      <City>ARLES</City>
      <StateOrProvince>bouches du rhone</StateOrProvince>
      <PostCode>13200 </PostCode>
      <Country>FRANCE</Country>
    </ContactAddress>
    <ContactVoiceTelephone>04 67 54 87 30</ContactVoiceTelephone>
    <ContactElectronicMailAddress>vincent.heurteaux@geomatys.fr</ContactElectronicMailAddress>
  </ContactInformation>
  <!-- Fees or access constraints imposed. -->
  <Fees>none</Fees>
